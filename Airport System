#include <stdio.h>
#include <string.h>

int main() {

    char cmd;
    getchar(cmd);

    switch (cmd) {
        case 'A': adiciona_aero()
        case 'C': encerra_aero()
        case 'F': adiciona_voo()
        case 'G': adiciona_rota()
        case 'I': altera_capacidade()
        case 'L': listagem_aero()
        case 'N': numero_voos()
        case 'O': abre_aero()
        case 'P': aero_mais_voos()
        case 'Q': aero_mais_conectado()
        case 'R': remove_voo()
        case 'S': remove_voo_ida_volta()
        case 'V': popularidade_voo()
        case 'X': exit()
    }
    return 0

}

